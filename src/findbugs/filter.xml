<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
<!--  exclude all categories except CORRECTNESS, STYLE, and BAD_PRACTICE -->
<Match>
  <Bug category="EXPERIMENTAL,PERFORMANCE,MT_CORRECTNESS" />
</Match>
<!--  exclude specific bugs from HICSS paper -->
<Match>
  <Bug pattern="IM_BAD_CHECK_FOR_ODD,DMI_USELESS_SUBSTRING,RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE,NM_METHOD_NAMING_CONVENTION,HE_EQUALS_USE_HASHCODE" />
</Match>
<!--  exclude specific bugs from Web-CAT analysis -->
<!-- Excluded Warnings from Correctness: -->
<Match>
  <Bug pattern="IJU_SETUP_NO_SUPER,IJU_TEARDOWN_NO_SUPER,ME_ENUM_FIELD_SETTER,RV_ABSOLUTE_VALUE_OF_HASHCODE,NP_ALWAYS_NULL_EXCEPTION,HE_SIGNATURE_DECLARES_HASHING_OF_UNHASHABLE_CLASS"/>
</Match>
<!-- Excluded Warnings from Bad Practice: -->
<Match>
  <Bug pattern="VA_FORMAT_STRING_USES_NEWLINE,DM_EXIT,NM_CLASS_NAMING_CONVENTION,SE_COMPARATOR_SHOULD_BE_SERIALIZABLE,HE_EQUALS_NO_HASHCODE,NM_CLASS_NOT_EXCEPTION,HE_INHERITS_EQUALS_USE_HASHCODE,CNT_ROUGH_CONSTANT_VALUE,IT_NO_SUCH_ELEMENT,CN_IMPLEMENTS_CLONE_BUT_NOT_CLONEABLE,DMI_RANDOM_USED_ONLY_ONCE,NM_FIELD_NAMING_CONVENTION,DE_MIGHT_IGNORE,RV_NEGATING_RESULT_OF_COMPARETO,RC_REF_COMPARISON_BAD_PRACTICE_BOOLEAN,CN_IDIOM_NO_SUPER_CALL"/>
</Match>
<!-- Excluded Warnings from Style: -->
<Match>
  <Bug pattern="NP_NULL_ON_SOME_PATH_MIGHT_BE_INFEASIBLE,ICAST_QUESTIONABLE_UNSIGNED_RIGHT_SHIFT,EQ_UNUSUAL,IA_AMBIGUOUS_INVOCATION_OF_INHERITED_OR_OUTER_METHOD"/>
</Match>
</FindBugsFilter>
